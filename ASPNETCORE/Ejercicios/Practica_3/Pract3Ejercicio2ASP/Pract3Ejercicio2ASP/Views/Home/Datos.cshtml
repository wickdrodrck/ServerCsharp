@model Usuario

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{

    <h1>Datos xD</h1>
    <p>Nombre: @Model.Nombre</p>
    <p>Mensaje: @Model.Mensaje</p>
    <ul>Aficiones: 
    @for (var i = 0; i < @Model.Aficiones.Count(); i++)
    {
        <li>@Model.Aficiones[i].Nombre</li>
    }
    </ul>
}
