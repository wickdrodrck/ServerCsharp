@model ContraseñaOlvidar
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    string defaultEmail = Model?.email ?? "";
}

<form asp-action="OlvidarContra">
    e-mail: <input type="text" value="@defaultEmail" asp-for="email"/><br />
    contraseña <input type="password" asp-for="contra"/><br />
    repetir contraseña <input type="password" asp-for="ContraseñaRepetida"/><br />
    <input type="submit" value="Enviar" />

</form>